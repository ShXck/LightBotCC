package misc;

public class Util {
	
	public static String remove_last_char(String s) {
		if(!s.isEmpty()) return s.substring(0, s.length() - 1);
		else return s;
	}
	
	public static boolean contains_str(String[] arr, String element) {
		for(int i = 0; i < arr.length; i++) {
			if(arr[i].equals(element)) return true;
		}
		return false;
	}

}
