package interpreter;

public class Variable {
	
	private int var_value = null;
	private String var_id;
	
	public Variable(String id, int val) {
		var_value = val;
		var_id = id;
	}
	
	public Variable(String id) {
		var_id = id;
	}
	
	public void set_var_value(int val) {
		var_value = val;
	}
	
	public int get_var_value() {
		return var_value;
	}
	
	public String get_var_id() {
		return var_id;
	}
	
	public String toString() {
		return var_id + ": " + var_value;
		
	}

}
